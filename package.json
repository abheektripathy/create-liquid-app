{
  "name": "create-liquid-apps",
  "version": "1.0.5",
  "description": "Scaffold Avail Nexus demo apps with framework + auth + widgets choices",
  "bin": {
    "create-liquid-apps": "./dist/index.js"
  },
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "base",
    "utils",
    "types"
  ],
  "scripts": {
    "build": "bun build ./index.ts --outdir dist --target node --minify --sourcemap=external",
    "dev": "bun run ./index.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "avail",
    "nexus",
    "liquid",
    "scaffold",
    "create",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abheektripathy/create-liquid-app.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "degit": "^2.8.4",
    "execa": "^9.5.0",
    "fs-extra": "^11.3.2",
    "ora": "^8.0.1",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18",
    "bun": ">=1.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4"
  }
